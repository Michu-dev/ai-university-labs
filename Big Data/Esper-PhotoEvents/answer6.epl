create window BeautyPhotos#length(10) as PhotoEvent;
insert into BeautyPhotos select * from PhotoEvent where genre = 'Beauty';
@name('answer') select avg(f.iso + s.iso + t.iso) as mean, f.its as its_first, s.its as its_second, t.its as its_third
from pattern[ every f=BeautyPhotos(iso > 1000) -> s=BeautyPhotos(height > 200) -> t=BeautyPhotos(iso > 1000) where timer:within(5) and not BeautyPhotos(iso < 100) ];