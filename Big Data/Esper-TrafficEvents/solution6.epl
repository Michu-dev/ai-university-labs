create window TrafficEventWindow#length(10) as TrafficEvent;
insert into TrafficEventWindow select * from TrafficEvent;
@name('answer') select st.car as car, st.velocity as st_velocity, mid.velocity as mid_velocity, high.velocity as high_velocity, st.its as st_its from pattern[ every st=TrafficEventWindow -> mid=TrafficEventWindow(car=st.car and velocity > st.velocity) -> high=TrafficEventWindow(car=mid.car and velocity > mid.velocity) where timer:within(5) and not TrafficEventWindow(car=st.car and velocity < 10)];